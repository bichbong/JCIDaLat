<div class="ui container">
  <h1 class="ui header">{{t 'My Tickets'}}</h1>
  <div class="ui grid">
    <div class="row">
      <div class="sixteen wide column">
        {{#tabbed-navigation}}
          {{#link-to 'admin.users.view.tickets.list' 'upcoming' class='item'}}
            {{t 'Sự kiện sắp diễn ra'}}
          {{/link-to}}
          {{#link-to 'admin.users.view.tickets.list' 'past' class='item'}}
            {{t 'Sự kiện đã bỏ lỡ'}}
          {{/link-to}}
        {{/tabbed-navigation}}
      </div>
    </div>
    {{outlet}}
  </div>
</div>
